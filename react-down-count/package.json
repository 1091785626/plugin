{
  "name": "react-basic",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "dev": "npm run serve",
    "build": "npm run dist",
    "serve": "npm run lint && webpack-dev-server --config  webpack.config.dev.js --color --progress --open",
    "dist": "npm run remove:dist && npm run lint && webpack --config webpack.config.dist.js -p --progress --color",
